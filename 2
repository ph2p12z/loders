local selectedColor = Color3.new(0.647059, 0.192157, 0.533333) 
local defaultColor = Color3.new(1, 1, 1) 

local function showTab(tab)
	MiscTab.Visible = tab == Misc
	VisualsTab.Visible = tab == Visuals
	CombatTab.Visible = tab == Combat
	PrivateTab.Visible = tab == Private


	Settings.TextColor3 = defaultColor
	Misc.TextColor3 = defaultColor
	Visuals.TextColor3 = defaultColor
	Combat.TextColor3 = defaultColor
	Private.TextColor3 = defaultColor

	
	tab.TextColor3 = selectedColor
end

Settings.MouseButton1Click:Connect(function()
	showTab(Settings)
end)

Misc.MouseButton1Click:Connect(function()
	showTab(Misc)
end)

Visuals.MouseButton1Click:Connect(function()
	showTab(Visuals)
end)

Combat.MouseButton1Click:Connect(function()
	showTab(Combat)
end)

Private.MouseButton1Click:Connect(function()
	showTab(Private)
end)
local selectedColor = Color3.new(0.647059, 0.192157, 0.533333) 
local defaultColor = Color3.new(0.32549, 0.0156863, 0.258824) 

local function toggleColor(button)
	if button.BackgroundColor3 == defaultColor then
		button.BackgroundColor3 = selectedColor
	else
		button.BackgroundColor3 = defaultColor
	end
end


