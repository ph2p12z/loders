local function createSlider(slider, label, maxValue, step)
	local slidePart = slider:FindFirstChild("SlidePart")
	local valueLabel = Instance.new("TextLabel")
	valueLabel.Name = "ValueLabel"
	valueLabel.Parent = slider
	valueLabel.BackgroundColor3 = Color3.new(1, 1, 1)
	valueLabel.BackgroundTransparency = 1
	valueLabel.BorderColor3 = Color3.new(0, 0, 0)
	valueLabel.BorderSizePixel = 0
	valueLabel.Position = UDim2.new(0.9, 0, 0, 0)
	valueLabel.Size = UDim2.new(0, 30, 0, 17)
	valueLabel.Font = Enum.Font.SourceSans
	valueLabel.TextColor3 = Color3.new(1, 1, 1)
	valueLabel.TextSize = 14
	valueLabel.Text = tostring(0)

	local function updateSlider(mouseX)
		local sliderWidth = slider.AbsoluteSize.X
		local value = math.clamp((mouseX - slider.AbsolutePosition.X) / sliderWidth, 0, 1) * maxValue
		value = math.floor(value / step) * step
		valueLabel.Text = tostring(value)
		slidePart.Size = UDim2.new(value / maxValue, 0, 0, 17)
		slidePart.BackgroundColor3 = Color3.new(0.647059, 0.192157, 0.533333)
	end

	local dragging = false
	local moveConnection

	slider.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			updateSlider(input.Position.X)
			moveConnection = game:GetService("RunService").RenderStepped:Connect(function()
				if dragging then
					local mouse = game.Players.LocalPlayer:GetMouse()
					updateSlider(mouse.X)
				end
			end)
		end
	end)

	slider.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = false
			if moveConnection then
				moveConnection:Disconnect()
			end
		end
	end)

	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			updateSlider(input.Position.X)
		end
	end)
end

createSlider(XSlider, X, 10, 1)
createSlider(YSlider, Y, 10, 1)
createSlider(ZSlider, Z, 10, 1)
createSlider(LongneckSlider, LN, 8, 1)
createSlider(FovSlider, Fov, 300, 1)
createSlider(FirerateSlider, label, 100, 1)
createSlider(SpreadSlider, label_2, 100, 1)
createSlider(RecoilSlider, label_3, 100, 1)
createSlider(XoffsetSlider, label_4, 5, 1)
createSlider(Yoffset, label_5, 5, 1)
createSlider(zoffsetSlider, label_6, 5, 1)
createSlider(ratioSlider, label_7, 100, 1)
createSlider(SpeedSlider, Speed, 90, 1)
createSlider(TransparencySlider, Transparency, 1, 0.100)
createSlider(LifetimeSlider, Lifetime, 5, 1)
local function setKeybind(keybindButton)
	local listening = false

	keybindButton.MouseButton1Click:Connect(function()
		if not listening then
			listening = true
			keybindButton.Text = "[...]"
		end
	end)

	game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
		if listening and not gameProcessed then
			if input.KeyCode ~= Enum.KeyCode.Unknown then
				keybindButton.Text = "[" .. input.KeyCode.Name .. "]"
				listening = false
			end
		end
	end)
end

setKeybind(LootAllKeybind)
setKeybind(AtvKeybind)
setKeybind(XrayKeybind)
setKeybind(LongneckKeybind)


LootAllKeybind.Text = "[None]"
AtvKeybind.Text = "[None]"
XrayKeybind.Text = "[None]"
LongneckKeybind.Text = "[None]"
local function CreateCustomCharacter(Mesh, Texture, Size)
    local part = Instance.new("Part", workspace)
    part.CFrame = workspace.Ignore.FPSArms.HumanoidRootPart.CFrame
    part.CanCollide = false
    part.Name = "DeleteMeForNone"
    local mesh = Instance.new("SpecialMesh", part)
    mesh.MeshType = Enum.MeshType.FileMesh
    mesh.Scale = Size
    mesh.TextureId = "http://www.roblox.com/asset/?id=" .. Texture
    mesh.MeshId = "http://www.roblox.com/asset/?id=" .. Mesh
    local weld = Instance.new("Weld", mesh)
    weld.Part0 = workspace.Ignore.FPSArms.HumanoidRootPart
    weld.Part1 = part
end

local CharacterType = "AnimeGirl"
local CustomCharacter = false
local function UpdateCharacter()
    for _, child in pairs(workspace:GetChildren()) do
        if child.Name == "DeleteMeForNone" then
            child:Destroy()
        end
    end
    if not CustomCharacter then
        return
    end

    local characterData = {
        AnimeGirl = { Mesh = 752599066, Texture = 752599541, Size = Vector3.new(0.7, 0.7, 0.7) }
    }
    local character = characterData[CharacterType]
    if character then
        CreateCustomCharacter(character.Mesh, character.Texture, character.Size)
    end
end
local function ToggleCustomCharacter(value)
    CustomCharacter = value
    UpdateCharacter()
end
ModelChangerToggle.MouseButton1Click:Connect(function()
    toggleColor(ModelChangerToggle)
    ToggleCustomCharacter(ModelChangerToggle.BackgroundColor3 == selectedColor)
end)
